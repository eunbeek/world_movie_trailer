{
    "hosting": {
        "public": "build/web",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    },
    "functions": [
        {
            "source": "functions",
            "codebase": "default",
            "ignore": [
                "node_modules",
                ".git",
                "firebase-debug.log",
                "firebase-debug.*.log"
            ]
        }
    ],
    "storage": {
        "rules": "storage.rules"
    },
    "flutter": {
        "platforms": {
            "android": {
                "default": {
                    "projectId": "world-movie-trailer",
                    "appId": "1:917295366628:android:ce236cf93580522d0cc7f1",
                    "fileOutput": "android/app/google-services.json"
                }
            },
            "ios": {
                "default": {
                    "projectId": "world-movie-trailer",
                    "appId": "1:917295366628:ios:18f419886a592f220cc7f1",
                    "uploadDebugSymbols": false,
                    "fileOutput": "ios/Runner/GoogleService-Info.plist"
                }
            },
            "dart": {
                "lib/firebase_options.dart": {
                    "projectId": "world-movie-trailer",
                    "configurations": {
                        "android": "1:917295366628:android:ce236cf93580522d0cc7f1",
                        "ios": "1:917295366628:ios:18f419886a592f220cc7f1"
                    }
                }
            }
        }
    }
}